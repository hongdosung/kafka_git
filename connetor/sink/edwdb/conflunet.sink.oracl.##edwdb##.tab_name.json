{
    "name": "conflunt.sink.oracle.##edwdb##tab_name_001",
    "config": {
         "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
         "tasks.max": "1",
         "topics": "##topic-prefix-oggbd-edwdb##EDWDB.tab_name_0002",
         "connection.url": "##tns-thin-edwdb##",
         "connection.user": "##edwdb-user##",
         "connection.password": "##edwdb-passwd##",
         "insert.mode": "upsert",
         "table.name.format": "EDWDB.TAB_NAME_001",
         "auto.create": "false",
         "auto.evolve": "false",
         "delete.enabled": "true",
         "pk.mode": "record_key",
         "db.timezone": "Asia/Seoul",
         "transforms": "ReplaceField,InsertField",
         "transforms.ReplaceField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
         "transforms.ReplaceField.exclude": "__OP_TYPE",
         "transforms.InsertField.type": "org.apache.kafka.connect.transforms.InsertField$Value",
         "transforms.InsertField.partition.field": "KFK_PTT",
         "transforms.InsertField.offset.field": "KFK_PTT",
         "transforms.InsertField.timestamp.field": "KFK_PRDC_TS",
         "key.converter": "org.apache.kafka.connect.json.JsonConverter",         
         "value.converter": "org.apache.kafka.connect.json.JsonConverter",         
         "key.converter.schemas.enable": "true",         
         "value.converter.schemas.enable": "true",
         "errors.retry.timeout": "0",
         "errors.log.enable": "true",
         "errors.log.include.messages": "true",
         "quote.sql.identifiers": "never",
         "auto.offset.reset": "earliest"
    }
}